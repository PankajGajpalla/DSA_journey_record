class Solution {
public:
    string reverseWords(string s) {
        int i=0;
        vector<string> ans;
        int n=s.length();
        while(i<s.length()){
            if(isspace(s[i])){
                i++;
                continue;
            }
            string temp = "";
            while(i<n && !isspace(s[i])){
                temp.push_back(s[i++]);
            }
            if(temp.length()>0) ans.push_back(temp);

        }

        string result = "";
        for(int i=ans.size()-1; i>0; i--){
            result += ans[i];
            result += " ";
        }
        result += ans[0];

        return result;
    }
};
