class Solution:
    def countGoodNumbers(self, n: int) -> int:
        # o = n//2
        # e = n-o # n//2 + n%2

        Mod = 1000000007
        def myPow(x, y):
            if y==0:
                return 1
            if y%2==0:
                return myPow((x*x)%Mod, y//2)
            return( x * myPow(x, y-1) ) % Mod
        
        return (myPow(5,n-(n//2)) * myPow(4,n//2) )% Mod

        
        
